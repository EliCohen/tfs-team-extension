{
  "name": "tfs-team-extension",
  "version": "1.0.0",
  "author": "Eli Cohen",
  "description": "Angular google chrome extension for TFS",
  "scripts": {
    "ng": "ng",
    "build:popup": "ng build",
    "build:popup-production": "ng build --prod",
    "watch:popup": "ng build --watch --delete-output-path false",
    "build:options": "ng build options",
    "build:options-production": "ng build options --prod",
    "watch:options": "ng build options --watch --delete-output-path false",
    "build:chrome": "webpack --config chrome/webpack.config.js",
    "build:chrome-production": "webpack -p --config chrome/webpack.config.prod.js",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "build": "npm run clean && npm run build:popup && npm run build:options && npm run build:chrome",
    "build:production": "npm run clean && npm run build:popup-production && npm run build:options-production && npm run build:chrome-production && npm run pack",
    "watch": "npm run clean && npm-run-all --parallel watch:*",
    "clean": "rimraf dist",
    "pack": "cd dist && bestzip ../../extension-build.zip *"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "core-js": "^3.6.4",
    "jquery": "^3.5.0",
    "ngx-spinner": "^8.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/chrome": "0.0.98",
    "@types/jquery": "^3.3.33",
    "@types/node": "^12.11.1",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.5",
    "codelyzer": "^5.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-extension-reloader": "^1.1.4"
  }
}
